<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>

  <!--log4net config-->
  <log4net>
    <appender name="TraceAppender" type="log4net.Appender.TraceAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %C{1}] - %message%newline" />
      </layout>
    </appender>
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="./logs/log.txt" />
      <appendToFile value="true" />
      <maximumFileSize value="10MB" />
      <maxSizeRollBackups value="6" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%date] [%level] [%thread] [%C{1}] - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="TraceAppender" />
      <appender-ref ref="RollingFile" />
    </root>
  </log4net>

  <appSettings>
    <add key="ClientApplicationServiceUrl" value="http://localhost:24313/ClientApplicationService.svc"/>
    <add key="PrintingServiceServiceUrl" value="http://localhost:24313/PrintingService.svc"/>
    <add key="ApplicationVersionFilePath" value="C:\wood\Develop\daan\daan.ui.PrintingApplication\bin\Debug\ReportTemplates\Versions.ini"/>
    <add key="LocalReportTemplatePath" value="C:\wood\Develop\daan\daan.ui.PrintingApplication\bin\Debug\ReportTemplates"/>
  </appSettings>
  <connectionStrings/>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
