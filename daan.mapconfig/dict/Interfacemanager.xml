<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="daan.domain" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ibatis.apache.org/mapping">
<alias>
	<typeAlias alias="Interfacemanager"  type="daan.domain.Interfacemanager,daan.domain" />
</alias>
<resultMaps>
		<resultMap id="InterfacemanagerResult" class="daan.domain.Interfacemanager"><result property="Interfacemanagerid" column="INTERFACEMANAGERID" type="single" dbType="Real"/><result property="Interfacename" column="INTERFACENAME" type="string" dbType="varchar"/><result property="Isstop" column="ISSTOP" type="string" dbType="varchar"/><result property="Status" column="STATUS" type="string" dbType="varchar"/><result property="Frequency" column="FREQUENCY" type="single" dbType="Real"/><result property="Starttime" column="STARTTIME" type="string" dbType="varchar"/><result property="Endtime" column="ENDTIME" type="string" dbType="varchar"/><result property="Remark" column="REMARK" type="string" dbType="varchar"/><result property="Displayorder" column="DISPLAYORDER" type="single" dbType="Real"/><result property="Createdate" column="CREATEDATE" type="DateTime" dbType="DateTime"/><result property="Executecode" column="EXECUTECODE" type="string" dbType="varchar"/></resultMap>
</resultMaps>
<statements>
	<select id="SelectInterfacemanager" parameterClass="string" resultMap="InterfacemanagerResult">SELECT INTERFACEMANAGERID AS Interfacemanagerid,INTERFACENAME AS Interfacename,ISSTOP AS Isstop,STATUS AS Status,FREQUENCY AS Frequency,STARTTIME AS Starttime,ENDTIME AS Endtime,REMARK AS Remark,DISPLAYORDER AS Displayorder,CREATEDATE AS Createdate,EXECUTECODE AS ExecutecodeFROM INTERFACEMANAGER<dynamic prepend="WHERE"><isParameterPresent>INTERFACEMANAGERID = #value#</isParameterPresent></dynamic></select><insert id="InsertInterfacemanager" parameterClass="Interfacemanager">	INSERT INTO INTERFACEMANAGER (INTERFACEMANAGERID,INTERFACENAME,ISSTOP,STATUS,FREQUENCY,STARTTIME,ENDTIME,REMARK,DISPLAYORDER,CREATEDATE,EXECUTECODE) 	VALUES(#Interfacemanagerid#,#Interfacename#,#Isstop#,#Status#,#Frequency#,#Starttime#,#Endtime#,#Remark#,#Displayorder#,#Createdate:DateTime:1/1/0001 12:00:00 AM#,#Executecode#)</insert><update id="UpdateInterfacemanager" parameterClass="Interfacemanager">UPDATE INTERFACEMANAGER  SET INTERFACEMANAGERID=#Interfacemanagerid#,INTERFACENAME=#Interfacename#,ISSTOP=#Isstop#,STATUS=#Status#,FREQUENCY=#Frequency#,STARTTIME=#Starttime#,ENDTIME=#Endtime#,REMARK=#Remark#,DISPLAYORDER=#Displayorder#,CREATEDATE=#Createdate#,EXECUTECODE=#Executecode#WHERE INTERFACEMANAGERID = #Interfacemanagerid#</update><delete id="DeleteInterfacemanager" parameterClass="string">DELETE FROM INTERFACEMANAGERWHERE INTERFACEMANAGERID = #value#</delete>
</statements>
</sqlMap>
